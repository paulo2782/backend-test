<form action="#" method="POST" id="register">
	<div class="container">
		<div class="card text-white bg-info">
	  <div class="card-header">Formulário Inscrição</div>      
	</div>
	    
	<div class="clearfix">&nbsp</div>

		<div class="row">
			<div class="col-lg-12">
				Nome
				<input type="text" class="form-control" name="name" id="name" required="true">
			</div>
			<div class="col-lg-12">
				E-mail
				<input type="text" class="form-control" name="email" id="email" required="true">
			</div>
			<div class="col-lg-12">
				Telefone
				<input type="text" class="form-control" name="phone" id="phone" required="true">
			</div>
		</div>
		
		<div class="clearfix">&nbsp</div>
		<div class="row">
			  <div class="col-lg-4">
				<input type="submit" value="CADASTRAR" class="btn btn-success form-control">
		  </div>
      <div class="col-lg-4">
        <input type="button" id="btn_end" value="FIM DA COMPETIÇÃO" class="btn btn-danger form-control">
      </div>
    </div>
		</div>

		<div class="clearfix">&nbsp</div>
		<div class="container">
		<table class="table table-inverse">
			<thead>
				<tr>
					<th>NOME</th>
					<th>E-MAIL</th>
					<th>LINK</th>
					<th>PONTOS</th>
				</tr>
			</thead>
			<tbody id="list">
				<tr>
					<td></td>
				</tr>
			</tbody>
		</table>
	</div>

